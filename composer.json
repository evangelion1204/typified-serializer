{
  "name": "evangelion1204/typified-serializer",
  "description": "A serializer to deep serialize all kind of classes and to deserialize them. Based on symfony/serializer",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Iwersen",
      "email": "mi.iwersen@gmail.com"
    }
  ],
  "require": {
    "symfony/serializer": "~2.7",
    "symfony/property-access": "~2.7",
    "phpunit/phpunit": "~4.7"
  },
  "require-dev": {
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "evangelion1204\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "tests\\evangelion1204\\": "./"
    }
  }
}
